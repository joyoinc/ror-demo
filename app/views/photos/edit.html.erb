<h1>Editing photo</h1>

<%= form_for(@photo, :url=>{:action=>'update'}) do |f| %>

  <%= f.label :token %>
  <%= f.text_field :token %>

  <%= f.label :tags %>
  <%= f.text_field :tags %>

  <%= f.submit "Ok", class: "btn btn-large btn-primary" %>

<% end %>

<%= link_to 'Show', @photo %> |
<%= link_to 'Back', photos_path %>
